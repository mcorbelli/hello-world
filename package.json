{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "npx dotenv -e .env.local -- npx @better-auth/cli generate --config lib/auth/server.ts",
    "build": "next build",
    "dev": "next dev --turbopack",
    "postinstall": "npx sort-package-json",
    "lint": "next lint",
    "prepare": "husky",
    "prisma:db:push": "npx dotenv -e .env.local -- npx prisma db push",
    "prisma:db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:deploy": "npx dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma:migrate:dev": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:reset": "npx dotenv -e .env.local -- npx prisma migrate reset",
    "prisma:studio": "npx dotenv -e .env.local -- npx prisma studio",
    "shadcn:update": "for file in components/ui/*.tsx; do npx shadcn@latest add -y -o $(basename '$file' .tsx); done",
    "start": "next start"
  },
  "resolutions": {
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.735.0",
    "@aws-sdk/s3-request-presigner": "^3.735.0",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "6.3.1",
    "@prisma/extension-accelerate": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@tanstack/react-table": "^8.21.2",
    "better-auth": "^1.2.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv-cli": "^8.0.0",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "lucide-react": "^0.477.0",
    "next": "15.2.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "sonner": "^1.7.4",
    "ua-parser-js": "^2.0.3",
    "uuid": "^11.1.0",
    "yup": "^1.6.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@faker-js/faker": "^9.5.1",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@types/lodash": "^4.17.14",
    "@types/node": "22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "class-variance-authority": "^0.7.1",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prisma": "6.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.6.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/db-seed.ts"
  }
}
